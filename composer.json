{
    "name": "zareismail/shaghool",
    "description": "A Laravel Nova tool.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0",
        "zareismail/nova-cascade": "~0.2",
        "zareismail/keil": "~0.1"
    },
    "autoload": {
        "psr-4": {
            "Zareismail\\Shaghool\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Zareismail\\Shaghool\\ShaghoolServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
